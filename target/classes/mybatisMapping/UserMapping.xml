<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyh.dao.myUser.IUserMapper">

    <select id="queryUserInfo" resultType="com.cyh.bean.MyUser">
        select
        address,
        age,
        name
        from
        my_user
